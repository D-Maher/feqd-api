<div>
<h1>New Deck Form</h1>
<div>
<h4>Create New Deck</h4>
  <%= form_for(@deck, url: admin_decks_path) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :description %>
    <%= f.text_area :description, size: "30x10" %>
    <%= f.submit "Create Deck" %>
  <%end%>
</div>
<%= render '/layouts/footer'%>
